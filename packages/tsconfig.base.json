{
  "compilerOptions": {
    "module": "CommonJS",
    "declaration": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": false,
    "importHelpers": true,
    "removeComments": false,
    "noLib": false,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ESNext",
    "sourceMap": false,
    "allowJs": true,
    "strict": true,
    "jsx": "react"
  },
  "exclude": [
    "../node_modules",
    "./**/*.spec.ts"
  ]
}